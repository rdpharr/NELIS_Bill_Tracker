<h2>{{title}}</h2>
{{#each data}}
    <hr>
    <h4><a href="https://www.leg.state.nv.us{{this.url}}">{{this.bill}}</a> - {{this.summary}}</h4>
    <p><b>Sponsor: </b>{{this.primary_sponsor}}</p>
    <table class="table table-striped table-bordered display responsive">
        <tr>
            <td><b>Title:</b></td>
            <td>{{this.title}}</td>
        </tr>
        <tr>
            <td><b>Digest:</b></td>
            <td>{{this.digest}}</td>
        </tr>
    </table>
{{/each}}